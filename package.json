{
  "name": "minimal-promise-pool",
  "version": "0.0.0-semantically-released",
  "description": "A minimal library for managing multiple promise instances (promise pool).",
  "keywords": [
    "async",
    "promise",
    "promise-pool"
  ],
  "repository": "git@github.com:WillBooster/minimal-promise-pool.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "dist/index.min.cjs",
  "module": "dist/index.min.mjs",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c && yarn build-d-ts && rm -Rf dist/__tests__",
    "build-d-ts": "yarn tsc --declaration --emitDeclarationOnly",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepublishOnly": "pinst --disable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "postpublish": "pinst --enable",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-numeric-separator": "7.14.5",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/eslint": "7.28.1",
    "@types/jest": "27.0.2",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.4.1",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "@willbooster/eslint-config-ts": "6.4.1",
    "@willbooster/prettier-config": "6.4.0",
    "@willbooster/renovate-config": "6.4.0",
    "babel-jest": "27.2.5",
    "core-js": "3.18.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.2",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.3",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "rollup": "2.58.0",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "18.0.0",
    "sort-package-json": "1.52.0",
    "typescript": "4.4.4"
  },
  "packageManager": "yarn@3.0.2"
}
