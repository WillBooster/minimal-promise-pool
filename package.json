{
  "name": "minimal-promise-pool",
  "version": "0.0.0-semantically-released",
  "description": "A minimal library for managing multiple promise instances (promise pool).",
  "keywords": [
    "async",
    "promise",
    "promise-pool"
  ],
  "repository": "github:WillBooster/minimal-promise-pool",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "dist/index.min.cjs",
  "module": "dist/index.min.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/jest": "27.4.1",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@willbooster/eslint-config-ts": "9.0.0",
    "@willbooster/prettier-config": "9.0.0",
    "@willbooster/renovate-config": "9.2.0",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "esbuild": "0.14.34",
    "esbuild-plugin-d.ts": "1.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.6.2",
    "semantic-release": "18.0.1",
    "sort-package-json": "1.55.0",
    "typescript": "4.6.3"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=12"
  }
}
