{
  "name": "minimal-promise-pool",
  "version": "0.0.0-semantically-released",
  "description": "A minimal library for managing multiple promise instances (promise pool).",
  "keywords": [
    "async",
    "promise",
    "promise-pool"
  ],
  "repository": "git@github.com:WillBooster/minimal-promise-pool.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "dist/index.min.cjs",
  "module": "dist/index.min.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "node esbuild.js",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepublishOnly": "pinst --disable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "postpublish": "pinst --enable",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.2.2",
    "@types/jest": "27.4.0",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.4.2",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@willbooster/eslint-config-ts": "6.5.0",
    "@willbooster/prettier-config": "6.5.0",
    "@willbooster/renovate-config": "6.5.0",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "esbuild": "0.14.10",
    "esbuild-plugin-d.ts": "1.1.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "prettier": "2.5.1",
    "semantic-release": "18.0.1",
    "sort-package-json": "1.53.1",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=12"
  },
  "packageManager": "yarn@3.2.0-rc.10"
}
