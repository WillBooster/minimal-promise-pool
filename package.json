{
  "name": "minimal-promise-pool",
  "version": "0.0.0-semantically-released",
  "description": "A minimal library for managing multiple promise instances (promise pool).",
  "keywords": [
    "async",
    "promise",
    "promise-pool"
  ],
  "repository": "github:WillBooster/minimal-promise-pool",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "check-for-ai": "yarn install > /dev/null && yarn format > /dev/null && yarn lint-fix --quiet && yarn typecheck && yarn test --silent",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky || true",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky || true",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test{-,/}fixtures/**\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.56.11",
    "@types/micromatch": "4.0.9",
    "@willbooster/eslint-config-ts": "11.2.2",
    "@willbooster/prettier-config": "10.0.1",
    "build-ts": "15.0.6",
    "conventional-changelog-conventionalcommits": "9.0.0",
    "eslint": "9.29.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import-x": "4.16.0",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "micromatch": "4.0.8",
    "pinst": "3.0.0",
    "prettier": "3.6.1",
    "semantic-release": "24.2.5",
    "sort-package-json": "3.3.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vitest": "3.2.4"
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
