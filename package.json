{
  "name": "minimal-promise-pool",
  "version": "0.0.0-semantically-released",
  "description": "A minimal library for managing multiple promise instances (promise pool).",
  "keywords": [
    "async",
    "promise",
    "promise-pool"
  ],
  "repository": "github:WillBooster/minimal-promise-pool",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "dependencies": {
    "@babel/runtime": "7.22.0"
  },
  "devDependencies": {
    "@types/eslint": "8.40.0",
    "@types/micromatch": "4.0.2",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@willbooster/eslint-config-ts": "10.1.0",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "build-ts": "5.8.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-class-members": "1.18.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.8",
    "semantic-release": "20.1.3",
    "sort-package-json": "2.4.1",
    "typescript": "5.0.4",
    "vitest": "0.31.1"
  },
  "packageManager": "yarn@4.0.0-rc.44",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
